<h1>Make your form here</h1>
<form action="/pirates" method="POST">
  
  <div>
    <label for="pirate_name">Pirate Name:</label> <input type="text" name="pirate[name]" id="pirate_name">
  </div>
  
  <div>
    <label for="pirate_weight">Pirate Weight:</label> <input type="text" name="pirate[weight]" id="pirate_weight">
  </div> 
  
  <div> 
    <label for="pirate_height">Pirate Height:</label> <input type="text" name="pirate[height]" id="pirate_height">
  </div>
  
  <div>
    <label for="ship_name_1">Ship One Name:</label> <input type="text" name="pirate[ships][][name]" id="ship_name_1">
  </div>
  
  <div>
    <label for="ship_type_1">Ship One Type:</label> <input type="text" name="pirate[ships][][type]" id="ship_type_1">
  </div> 
  
  <div>
    <label for="ship_booty_1">Ship One Booty:</label> <input type="text" name="pirate[ships][][booty]" id="ship_booty_1">
  </div>
  
  <div>
    <label for="ship_name_2">Ship Two Name:</label> <input type="text" name="pirate[ships][][name]" id="ship_name_2">
  </div>
  
  <div>
    <label for="ship_type_2">Ship Two Type:</label> <input type="text" name="pirate[ships][][type]" id="ship_type_2">
  </div>
  
  <div>
    <label for="ship_booty_2">Ship Two Booty:</label> <input type="text" name="pirate[ships][][booty]" id="ship_booty_2">
  </div>
  
  <button type="submit">Submit</button>
</form>